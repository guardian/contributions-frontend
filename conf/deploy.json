{
    "defaultStacks": ["membership"],
    "packages":{
        "contributions-frontend":{
            "type":"autoscaling",
            "data":{
                "bucket": "membership-dist",
                "publicReadAcl": false
            }
        }
    },
    "recipes":{
        "default":{
            "actionsBeforeApp": ["contributions-frontend.uploadArtifacts", "contributions-frontend.deploy"]
        },
        "artifactUploadOnly": {
           "actions": [ "contributions-frontend.uploadArtifacts" ]
       }
    }
}
